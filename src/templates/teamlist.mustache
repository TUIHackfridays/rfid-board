{{#team}}
	<div class="col-sm-6 col-md-4 col-lg-3">
		<a href="https://github.com/{{github}}" class="thumbnail thumbnail-round" target="_blank">
			<img src="{{avatar}}" alt="{{name}}">
		</a>

		<br><strong>{{name}}</strong>
		<br>

		<icons>
			{{#github}}<a href="https://github.com/{{github}}" target="_blank"><span class="ion-social-github"></span></a>{{/github}}
			{{#twitter}}<a href="https://twitter.com/{{twitter}}" target="_blank"><span class="ion-social-twitter"></span></a>{{/twitter}}
			{{#skype}}<a href="skype:{{skype}}"><span class="ion-social-skype"></span></a>{{/skype}}
			{{#email}}<a href="mailto:{{email}}"><span class="ion-ios7-email"></span></a>{{/email}}
		</icons>

	</div>
{{/team}}
